{
    "name": "tackphp/social",
    "description": "A Package for Interacting with Iranian Social Media APIs",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Social\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Social\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Ahmadreza Bashari",
            "email": "ahmadrezabashari@gmail.com"
        }
    ],
    "require-dev": {
        "pestphp/pest": "^3.5",
        "vlucas/phpdotenv": "^5.6",
        "symfony/var-dumper": "^7.1",
        "mockery/mockery": "^1.6"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "require": {
        "guzzlehttp/guzzle": "^7.9"
    }
}
